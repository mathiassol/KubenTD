{
  "name": "your-app-name",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . YourAppName --platform=win32 --arch=x64 --out=dist --overwrite",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.yourapp.id",
    "productName": "YourAppName",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.0.0"
  },
  "dependencies": {
    "configparser": "^0.3.10",
    "ini": "^5.0.0"
  }
}